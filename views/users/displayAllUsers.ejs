<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" />
<div>
    <% if(rs.length > 0) { %>
    <table border="1">
        <tr><th>Name</th></tr>
        <% for (var i=0; rs.length > i; i++) { %>
        <tr>
            <td><a href="/users/?user_id=<%= rs[i].user_id %>"> <%= rs[i].firstname %>&nbsp;<%= rs[i].lastname %></a></td>
        </tr>
        <% } %>
    </table>
    <% } else { %>
    No users are currently available!
    <% } %>
</div>

<div style="margin-top:10px;">
    <a href="/signup">Add To the List! (Create A New Account)</a>
</div>